<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FSL_FTFR_Count__c</fullName>
    <description>Sets the value of the field to 0 if status is &#39;Cannot Complete&#39;. This field is used in Service Manager report to calculate First Time Fix Rate value</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Status,&#39;Cannot Complete&#39;),0,100)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>FTFR Count</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
