    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Flat</fullName>
                <default>false</default>
                <label>Flat</label>
            </value>
            <value>
                <fullName>Pitched</fullName>
                <default>false</default>
                <label>Pitched</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
