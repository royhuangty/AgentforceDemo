<?xml version="1.0" encoding="UTF-8"?>
<LiveChatSensitiveDataRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionType>Replace</actionType>
    <description>What: Block sensitive credit card data.</description>
    <enforceOn>7</enforceOn>
    <isEnabled>true</isEnabled>
    <pattern>\b(?:\d{4})(?:-?|\040?)((((?:\d{4})(?:-?|\040?)){3})|(\d{6}(?:-?|\040?)(\d{5}|\d{4})))\b</pattern>
    <priority>2</priority>
    <processingType>RealTime</processingType>
    <replacement>XXXX-XXXX-XXXX-XXXX</replacement>
    <version>ONE</version>
</LiveChatSensitiveDataRule>
