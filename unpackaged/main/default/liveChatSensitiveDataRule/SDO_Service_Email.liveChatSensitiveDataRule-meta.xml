<?xml version="1.0" encoding="UTF-8"?>
<LiveChatSensitiveDataRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionType>Replace</actionType>
    <description>What: Block sensitive email information.</description>
    <enforceOn>7</enforceOn>
    <isEnabled>false</isEnabled>
    <pattern>\b([a-z0-9!#$%&amp;&#39;*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&amp;&#39;*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?)\b</pattern>
    <priority>2</priority>
    <processingType>RealTime</processingType>
    <replacement>&lt;Email&gt;</replacement>
    <version>ONE</version>
</LiveChatSensitiveDataRule>
