        "components" : [ {
          "componentAttributes" : {
            "richTextValue" : "<h2>Deal Registration Guidelines</h2>"
          },
          "componentName" : "forceCommunity:richTextInline",
          "id" : "df6914e1-3f29-45c7-8381-c8a8504c9e73",
          "renderPriority" : "NEUTRAL",
          "renditionMap" : { },
          "type" : "component"
        }, {
          "componentAttributes" : {
            "richTextValue" : "<h4><strong>Eligibility:</strong></h4><ul><li>The opportunity is for direct partners/resellers only.</li><li>The opportunity is not registered by another partner.</li><li>The partner has made significant pre-sales efforts related to the deal, such as meeting with decision-makers, qualifying the opportunity, helping the customer quantify the project budget with Pacifica products or services, and assisting the customer in defining project requirements to include Pacifica products or services.</li></ul><p><br></p><h4>&nbsp;<strong>Dispute Resolution:</strong></h4><ul><li>Submit any disputes arising from this deal registration program via the portal experience.</li></ul>"
          },
          "componentName" : "forceCommunity:richTextInline",
          "id" : "7c0f70ce-a117-47ea-b05b-f30cc6858369",
          "renderPriority" : "NEUTRAL",
          "renditionMap" : { },
          "type" : "component"
        } ],
        "id" : "92154e3b-7e04-48bb-8244-0e3e6fb9d132",
        "regionLabel" : "Column 1",
        "regionName" : "1",
        "renditionMap" : { },
        "type" : "region"
      }, {
        "components" : [ {
          "componentAttributes" : {
            "richTextValue" : "<h2><br></h2>"
          },
          "componentName" : "forceCommunity:richTextInline",
          "id" : "da16e0ed-6ca4-48ac-8d68-058ec97fea6a",
          "renderPriority" : "NEUTRAL",
          "renditionMap" : { },
          "type" : "component"
        }, {
          "componentAttributes" : {
            "richTextValue" : "<h4><strong>Registration Process:</strong></h4><ul><li>Submit the deal for review via the deal registration form [here].</li><li>The first partner to submit a deal meeting all eligibility criteria will be approved for that customer opportunity.</li><li>Approved partners will be notified via the portal experience.</li><li>Declined partners will be notified via the portal experience, including reasons for the decision.</li></ul><p><br></p><h4>&nbsp;<strong>Deal Protection:</strong></h4><ul><li>Approved deals will be protected for three months from the date of approval.</li><li>If a material change to the solution is proposed, a new registration for the opportunity must be submitted.</li></ul>"
          },
